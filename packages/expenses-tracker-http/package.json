{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@repo/expenses-tracker-http",
  "type": "module",
  "version": "0.1.0",
  "description": "HTTP API definitions defined with HTTP module from effect/platform.",
  "scripts": {
    "build": "tsup",
    "check": "biome check --write ./src",
    "format": "biome format --write ./src",
    "lint": "biome lint --write ./src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@effect/platform": "^0.71.6",
    "effect": "^3.11.9"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./checkhealth": {
      "types": "./src/CheckHealth.ts",
      "default": "./dist/CheckHealth.js"
    },
    "./domain": {
      "types": "./src/Domain.ts",
      "default": "./dist/Domain.js"
    },
    "./errors": {
      "types": "./src/Errors.ts",
      "default": "./dist/Errors.js"
    }
  },
  "devDependencies": {
    "@repo/biome-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "esbuild": "0.24.0",
    "tsup": "^8.3.5"
  }
}
